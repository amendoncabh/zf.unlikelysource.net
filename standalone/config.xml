<?xml version="1.0"?>
<configdata>
	<production>
		<webhost>www.example.com</webhost>
		<database>
			<adapter>pdo_mysql</adapter>
			<params>
				<host>db.example.com</host>
				<user>dbuser</user>
				<pwd>secret</pwd>
				<dbname>dbname</dbname>
			</params>
		</database>
	</production>
	<staging extends="production">
		<database>
			<params>
				<host>dev.example.com</host>
				<user>devuser</user>
				<pwd>devsecret</pwd>
			</params>
		</database>
	</staging>
	<other>
		<color>
			<red>Red</red>
			<green>Green</green>
			<blue>Blue</blue>
		</color>
		<capitol>
			<us>Washington</us>
			<in>New Delhi</in>
			<au>Canberra</au>
			<cn>Ottawa</cn>
			<de>Berlin</de>
		</capitol>
	</other>
</configdata>

